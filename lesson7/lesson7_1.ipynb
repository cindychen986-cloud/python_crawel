    "import nest_asyncio\n",
    "import asyncio\n",
    "from crawl4ai import AsyncWebCrawler\n",
    "nest_asyncio.apply()\n",
    "\n",
    "async def main():\n",
    "    async with AsyncWebCrawler() as crawler:\n",
    "        result = await crawler.arun(\n",
    "            url=\"https://example.com\")\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    asyncio.run(main())"
   ]
  }
